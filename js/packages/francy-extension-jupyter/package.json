{
  "name": "francy-extension-jupyter",
  "version": "1.0.4",
  "description": "A package for rendering application/vnd.francy+json in Jupyter",
  "author": "Manuel Martins <manuelmachadomartins@gmail.com>",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcmartins/francy.git"
  },
  "license": "MIT",
  "keywords": [
    "gap",
    "francy",
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "jupyter notebook",
    "jupyter notebook extension"
  ],
  "jupyterlab": {
    "mimeExtension": "lib/lab_extension"
  },
  "scripts": {
    "clean": "rm -rf ./lib ./dist ./build ./jupyter_francy/jupyter_francy ./jupyter_francy/__pycache__ ./jupyter_francy/nbextension ./jupyter_francy/labextension ./jupyter_francy.egg-info ./__pycache__",
    "build:lib": "babel src -d lib",
    "build:extension": "webpack --config webpack.config.js",
    "build": "npm run clean && npm run build:lib && npm run build:extension",
    "build:production": "npm run clean && npm run build:lib && webpack --env.production --config webpack.config.js"
  },
  "dependencies": {
    "d3": "5.9.2",
    "d3-graphviz": "2.6.1",
    "francy": "^1.0.4",
    "francy-core": "^1.0.4",
    "francy-renderer-d3": "^1.0.4",
    "francy-renderer-graphviz": "^1.0.4",
    "viz.js": "1.8.1"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-decorators": "7.1.2",
    "@babel/plugin-transform-classes": "7.1.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@phosphor/widgets": "1.6.0",
    "babel-loader": "8.0.4",
    "css-loader": "1.0.0",
    "del": "3.0.0",
    "file-loader": "2.0.0",
    "json-loader": "0.5.7",
    "lodash.isequal": "4.5.0",
    "mkdirp": "0.5.1",
    "npm-run-all": "4.1.3",
    "rimraf": "2.6.2",
    "style-loader": "0.23.1",
    "url-loader": "1.1.1",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.2",
    "replace": "1.1.0"
  }
}
